void main() {
  // Example 1: Integer (int)
  int numberOfStudents = 150;
  print('Number of students: $numberOfStudents');

  // Example 2: Double (double)
  double averageScore = 85.5;
  print('Average score: $averageScore');

  // Example 3: Integer and Double operations
  int totalApples = 10;
  double pricePerApple = 1.25;
  double totalCost = totalApples * pricePerApple;
  print('Total cost for $totalApples apples: \$${totalCost.toStringAsFixed(2)}');
}
