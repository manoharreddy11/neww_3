void main(){

  // numbers: int
  int age = 10;
  var age = 10;
  
  // numbers : double
  double price = 10.50;
  var price = 10.50;
  
  
  // Strings
  String name = "Manohar Reddy";
  String you = 'single quotes';
  var you = 'single quotes'; 
  
  // boolean
  
  bool alive;
  bool dead = false;
  
