//example1
enum Day {
  Monday,
  Tuesday,
  Wednesday,
  Thursday,
  Friday,
  Saturday,
  Sunday
}

void main() {
  print(Day.Monday); 
  print(Day.Friday); 
}

//example2
enum Status {
  success,
  error,
  inProgress
}

void printStatus(Status status) {
  switch (status) {
    case Status.success:
      print('Operation was successful!');
      break;
    case Status.error:
      print('An error occurred.');
      break;
    case Status.inProgress:
      print('Operation is in progress.');
      break;
  }
}

void main() {
  printStatus(Status.success); 
}

